{
  "$schema": "https://deno.land/x/denon@2.5.0/schema.json",
  "scripts": {
    "start": {
      "cmd": "deno run src/main.ts",
      "allow": {
        "env": true,
        "read": ".env,.env.defaults",
        "net": true
      }
    },
    "test": {
      "cmd": "deno test",
      "allow": {
        "env": true,
        "read": ".env.test,.env.defaults",
        "net": true
      }
    }
  }
}
